#!/usr/bin/env php
<?php

namespace Qafoo\Bsoad;

require __DIR__ . '/../main/Qafoo/Bsoad/bootstrap.php';

$reader = new Reader\Tcpdump(
    new Sorter\Basic(
        new Writer\Dispatcher( array(
            new Writer\Json( STDOUT ),
            new Writer\Debug( STDERR ),
        ) )
    )
);

$reader->process( STDIN );

